{
  "name": "TEKEVER-API",
  "version": "2.0.38-RELEASE",
  "description": " TEKEVER API",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:hml": "env-cmd -f .env nest build",
    "new-build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f .env nest start",
    "start:dev": "env-cmd -f dev.env nest start --watch",
    "start:debug": "env-cmd -f dev.env nest start --debug --watch",
    "start:prod": "env-cmd -f prod.env nest start",
    "start:migrate:prod": "npx prisma generate && npm run start",
    "migrate-init": "npx prisma migrate dev --name init",
    "migrate-generate": "npx prisma generate",
    "migrate-dev": "npx prisma migrate dev",
    "migrate-push": "npx prisma db push",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --setupFiles dotenv/config",
    "test:watch": "jest  --setupFiles dotenv/config --watch",
    "test:cov": "jest  --setupFiles dotenv/config --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.9",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.9",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^8.0.9",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.1.0",
    "@prisma/client": "3.13.0",
    "axios": "^0.21.1",
    "bson": "^5.0.0",
    "cache-manager": "4.1.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.1",
    "env-cmd": "^10.1.0",
    "keycloak-connect": "^15.0.2",
    "moment": "^2.24.0",
    "nest-keycloak-connect": "^1.7.6",
    "nestjs-cls": "^2.0.1",
    "nestjs-redoc": "^2.2.2",
    "prisma": "3.13.0",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^9.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^8.1.2",
    "@nestjs/testing": "^8.1.1",
    "@types/cache-manager": "4.0.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "faker-br": "^0.4.1",
    "jest": "^26.6.3",
    "jest-mock-extended": "2.0.4",
    "nock": "^13.3.2",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "globalSetup": "./dist/tests/jest-config.js",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testMatch": [
      "**/dist/**/*.test.js"
    ],
    "testEnvironment": "node"
  }
}
